<main class="contenedor asistentes">
    <h1><%= pageName %></h1>

    <% if(assistants.length > 0 ) { %>
        <ul class="listado-asistentes">
            <% assistants.forEach(assistant => {Â  %>
                <li>
                    <div class="imagen">
                        <% if(assistant.image) { %> 
                            <img src="/uploads/profiles/<%= assistant.image %>">
                        <% } else { %>
                            <div class="circulo-gris"></div>
                        <% } %>
                    </div>
                    <div class="texto">
                        <%= assistant.name %>
                    </div>
                </li>
            <% }) %>
        </ul>
    <% } else { %>
        <p class="no-asistentes">No hay asistentes</p>
    <% } %>
</main>