<div class="contenedor">
    <h1><%= pageName %></h1>
    <% if (eventsList.length > 0 ) { %>
    <div class="grid columnas-md-3 ">
        <% eventsList.forEach(event => { %>
            <%- include ('partials/card-event.ejs', { event: event }) %>
        <% }) %>
    </div><!--grid-->
    <% } else { %>
        <p>No hay eventos en esta categoría</p>
    <% } %>
</div>